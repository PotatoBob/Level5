import java.util.Stack;

public class AdvancedPuzzles {
	static Stack<Integer> sequence;
	static String list;
	public static void main(String[] args) {
		sequence(18);
	}
	static void sequence(int a) {
		while(a>=0) {
			System.out.println(a);
			if(a%2==0) {
				a/=2;
			} else {
				a--;
				a/=2;
			}
		}
	}
}
