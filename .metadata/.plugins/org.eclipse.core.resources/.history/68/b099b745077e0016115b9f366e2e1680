package Hospital;

public class Patient {
	boolean caredFor = false;
	boolean isAlive = true;
	
	boolean getCaredFor() {
		return caredFor;
	}
	void checkPulse() {
		caredFor = true;
	}
	boolean feelsCaredFor() {
		return getCaredFor();
	}
	boolean getisAlive() {
		return isAlive;
	}
	boolean isAlive() {
		return getisAlive();
	}
	void kill() {
		isAlive = false;
	}
}
