

public class Primes {
		int[] primes = new int[1000];
		boolean running = true;
		int count = 0;
		int num = 2;
	public static void main(String[] args) throws InterruptedException {
		Primes P = new Primes();
	}
	Primes() throws InterruptedException{
		Thread t = new Thread();
		new Thread(()-> {
			while(running) {
				if(isPrime(num)) {
					primes[count]=num;
					count++;
				}
				if(count==1000) {
					running=false;
				}
				num++;
			}
		}).start();
		t.join();
		for(int k : primes) {
			System.out.println(k);
		}
	}
	boolean isPrime(int n) {
		for(int i = 2; i<n; i++) {
			if (n%i==0) {
				return false;
			}
		}
		return true;
	}
}
