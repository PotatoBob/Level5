import java.util.Stack;

public class AdvancedPuzzles {
	static Stack<Integer> stuff;
	static String list;
	static int x;
	public static void main(String[] args) {
		sequence(18);
	}
	static void sequence(int a) {
		while(a>0) {
			x=a;
			stuff.push(x);
			if(a%2==0) {
				a/=2;
			} else {
				a--;
				a/=2;
			}
		}
		System.out.println(a);
	}
}
