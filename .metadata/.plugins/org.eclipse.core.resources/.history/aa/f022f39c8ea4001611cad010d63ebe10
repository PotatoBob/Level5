
public class Primes {
	public static void main(String[] args) {
		int[] primes = new int[1000];
		boolean running = true;
		int count = 0;
		int num = 2;
		new Thread(()-> {
			while(running) {
				if(isPrime(num)) {
					
				}
			}
		}).start();
	}
	static boolean isPrime(int n) {
		for(int i = 2; i<Math.sqrt(n); i++) {
			if (n%i==0) {
				return false;
			}
		}
		return true;
	}
}
