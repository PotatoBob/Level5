import java.util.Stack;

public class AdvancedPuzzles {
	Stack<Integer> sequence;
	String list;
	public static void main(String[] args) {
		
	}
	void Sequence(int a) {
		while(a>0) {
			sequence.push(a);
			if(a%2==0) {
				a/=2;
			} else {
				a=(a-1)/2;
			}
		}
		sequence.push(a);
		for(int i = 0; i<sequence.size()-1; i++) {
			list=list+sequence.pop()+", ";
		}
		list=list+sequence.pop()+"";
	}
}
