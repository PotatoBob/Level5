import java.util.Stack;

public class AdvancedPuzzles {
	static Stack<Integer> stuff;
	static String list;
	static int x;
	public static void main(String[] args) {
		sequence(18);
	}
	static void sequence(int a) {
		stuff=new Stack<Integer>();
		while(a>0) {
			stuff.push(a);
			if(a%2==0) {
				a/=2;
			} else {
				a--;
				a/=2;
			}
		}
		stuff.push(a);
		x=stuff.size();
		for(int i = 0; i<x-1; i++) {
			System.out.print(stuff.pop()+", ");
		}
		System.out.println(stuff.pop());
	}
}
